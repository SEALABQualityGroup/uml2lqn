<?xml version="1.0" encoding="UTF-8"?>
<lqn-model name="Model">
  <processor name="container-web" replication="1">
    <task name="web">
      <entry name="web-bes">
        <entry-phase-activities>
          <activity host-demand-mean="20.0" name="findProducts" phase="1">
            <synch-call dest="prod1-bes" calls-mean="1775.0"/>
          </activity>
          <activity host-demand-mean="15.0" name="findItems" phase="1">
            <synch-call dest="item1-bes" calls-mean="1903.0"/>
          </activity>
          <activity host-demand-mean="24.0" name="getcategory" phase="1">
            <synch-call dest="cat-bes" calls-mean="2605.0"/>
          </activity>
          <activity host-demand-mean="14.0" name="findFeatures" phase="1">
            <synch-call dest="item2-bes" calls-mean="2890.0"/>
          </activity>
          <activity host-demand-mean="16.0" name="findItemsRandom" phase="1">
            <synch-call dest="item3-bes" calls-mean="2089.0"/>
          </activity>
          <activity host-demand-mean="16.0" name="findProductsRandom" phase="1">
            <synch-call dest="prod2-bes" calls-mean="1700.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
    <task name="User" scheduling="ref">
      <entry name="user-bes">
        <entry-phase-activities>
          <activity host-demand-mean="12.0" name="getHomepageMsg" phase="1">
            <synch-call dest="web-bes" calls-mean="202601.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
  <processor name="container-categories" replication="1">
    <task name="categories">
      <entry name="cat-bes">
        <entry-phase-activities>
          <activity host-demand-mean="24.0" name="getcategory" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
  <processor name="container-items" replication="1">
    <task name="items">
      <entry name="item3-bes">
        <entry-phase-activities>
          <activity host-demand-mean="16.0" name="findItemsRandom" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="item2-bes">
        <entry-phase-activities>
          <activity host-demand-mean="14.0" name="findFeatures" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="item1-bes">
        <entry-phase-activities>
          <activity host-demand-mean="15.0" name="findItems" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="navigate-items-bes">
        <entry-phase-activities>
          <activity host-demand-mean="15.0" name="findItemsRandomByProductId" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
    <task name="Mobile-User" scheduling="ref">
      <entry name="mobile-user-items">
        <entry-phase-activities>
          <activity host-demand-mean="15.0" name="findItemsRandomByProductId" phase="1">
            <synch-call dest="navigate-items-bes" calls-mean="20.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
  <processor name="container-products" replication="1">
    <task name="products">
      <entry name="prod2-bes">
        <entry-phase-activities>
          <activity host-demand-mean="16.0" name="findProductsRandom" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="prod1-bes">
        <entry-phase-activities>
          <activity host-demand-mean="20.0" name="findProducts" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="show-product-bes">
        <entry-phase-activities>
          <activity host-demand-mean="18.0" name="findProductShow" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
    <task name="Warehouse worker" scheduling="ref">
      <entry name="">
        <entry-phase-activities>
          <activity host-demand-mean="18.0" name="findProductShow" phase="1">
            <synch-call dest="show-product-bes" calls-mean="2000.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
</lqn-model>
