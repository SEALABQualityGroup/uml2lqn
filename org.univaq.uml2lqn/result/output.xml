          <activity name="controller_behavior">
            <synch-call dest="get_cond_2" calls-mean="1.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
    <task name="user_request" scheduling="ref"/>
  </processor>
</LqnModelType>
