<?xml version="1.0" encoding="UTF-8"?>
<LqnModelType name="PrecisionAgricultureModel">
  <processor name="ARMCortex-M" replication="3">
    <task name="env-sensor">
      <entry name="get_condition_message1" type="PH1PH2">
        <entry-phase-activities>
          <activity name="farm_behavior" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
  <processor name="nvidiaGeForce8800" replication="1">
    <task name="drone-controller">
      <entry name="start_drone1" type="PH1PH2">
        <entry-phase-activities>
          <activity name="controller_behavior" phase="1">
            <synch-call dest="get_condition_message1" calls-mean="1.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
    <task name="user_request" scheduling="ref">
      <entry name="init_start_drone1" type="PH1PH2">
        <entry-phase-activities/>
      </entry>
    </task>
  </processor>
</LqnModelType>
