<?xml version="1.0" encoding="UTF-8"?>
<LqnModelType name="automatedGuidedVehicle">
  <processor name="AutomatedGuidedVehicle" replication="1">
    <task multiplicity="1" name="vehicle-control">
      <entry name="">
        <entry-phase-activities>
          <activity host-demand-mean="0.96" name="stop_message" phase="1">
            <synch-call dest="motor1_bes" calls-mean="1.0"/>
          </activity>
          <activity host-demand-mean="0.741" name="load_message" phase="1">
            <synch-call dest="arm_bes" calls-mean="1.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
      <entry name="control0_bes">
        <entry-phase-activities>
          <activity host-demand-mean="0.159" name="start_motor_message" phase="1">
            <synch-call dest="motor0_bes" calls-mean="1.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
      <entry name="control2_bes">
        <entry-phase-activities>
          <activity host-demand-mean="0.654" name="get_status_message" phase="1">
            <synch-call dest="status_bes" calls-mean="1.0"/>
          </activity>
          <activity host-demand-mean="0.742" name="display_message" phase="1">
            <synch-call dest="display_bes" calls-mean="1.0"/>
          </activity>
        </entry-phase-activities>
      </entry>
    </task>
    <task multiplicity="1" name="vehicle-status"/>
    <task multiplicity="1" name="ArrivalSensor" scheduling="ref"/>
    <task multiplicity="1" name="SupervisorySystem" scheduling="ref"/>
    <task multiplicity="1" name="Clock" scheduling="ref"/>
  </processor>
  <processor name="Motor" replication="1">
    <task multiplicity="1" name="motor">
      <entry name="motor0_bes">
        <entry-phase-activities>
          <activity host-demand-mean="0.159" name="start_motor_message" phase="1"/>
        </entry-phase-activities>
      </entry>
      <entry name="motor1_bes">
        <entry-phase-activities>
          <activity host-demand-mean="0.96" name="stop_message" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
  <processor name="RobotArm" replication="1">
    <task multiplicity="1" name="robot-arm"/>
  </processor>
  <processor name="DisplaySystem" replication="1">
    <task multiplicity="1" name="display-system">
      <entry name="display_bes">
        <entry-phase-activities>
          <activity host-demand-mean="0.742" name="display_message" phase="1"/>
        </entry-phase-activities>
      </entry>
    </task>
  </processor>
</LqnModelType>
